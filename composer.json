{
    "name": "anteris-dev/enum-descriptions",
    "description": "Adds description attributes for enums.",
    "license": "MIT",
    "authors": [
        {
            "name": "Aidan Casey",
            "email": "aidan.casey@anteris.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "AnterisDev\\EnumDescriptions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnterisDev\\Tests\\EnumDescriptions\\": "tests/"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
